	Eprint = {1107.2715},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Instrumentation and Methods for Astrophysics, Computer Science - Graphics},
	Month = jul,
	Primaryclass = {astro-ph.IM},
	Title = {{Stereo pairs in Astrophysics}},
	Year = 2011}

