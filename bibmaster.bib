	Date-Added = {2011-03-18 00:01:35 -0400},
	Date-Modified = {2011-03-18 00:02:28 -0400},
	Doi = {10.1051/0004-6361/201014428},
	Eprint = {1009.1682},
	Journal = {\aap},
	Keywords = {galaxies: clusters: individual: NGC 1399, galaxies: elliptical and lenticular, cD, techniques: spectroscopic},
